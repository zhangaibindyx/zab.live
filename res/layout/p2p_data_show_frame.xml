<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#90000000"
    
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:text="gcid:"
            android:textColor="@android:color/holo_orange_dark" />

        <TextView
            android:id="@+id/gcid"
            android:textSize="12sp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@android:color/holo_orange_dark" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1" >

            <TextView
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:text="dl_limit:"
                android:textColor="@android:color/holo_green_dark"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/dl_limit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@android:color/holo_green_dark"
                android:textSize="12sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1" >

            <TextView
                android:layout_width="68dp"
                android:layout_height="wrap_content"
                android:text="dl_pos:"
                android:textColor="#FFFF00"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/download_pos"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#FFFF00"
                android:textSize="12sp" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1" >

            <TextView
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:text="dl_speed:"
                android:textColor="@android:color/holo_red_dark"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/dl_speed"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@android:color/holo_red_dark"
                android:textSize="12sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1" >

            <TextView
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:text="dl_ratio:"
                android:textColor="#EE00EE"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/dl_ratio"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#EE00EE"
                android:textSize="12sp" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1" >

            <TextView
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:text="file_size:"
                android:textColor="#B3EE3A"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/file_size"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#B3EE3A"
                android:textSize="12sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1" >

            <TextView
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:text="type:"
                android:textColor="#A020F0"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/type"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#A020F0"
                android:textSize="12sp" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1" >

            <TextView
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:text="urgent_start:"
                android:textColor="@android:color/holo_red_dark"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/urgent_start"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@android:color/holo_red_dark"
                android:textSize="12sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1" >

            <TextView
                android:layout_width="72dp"
                android:layout_height="wrap_content"
                android:text="urgent_end:"
                android:textColor="#00FF00"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/urgent_end"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#00FF00"
                android:textSize="12sp" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1" >

            <TextView
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:text="total_dl_limit:"
                android:textColor="@android:color/holo_green_dark"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/total_dl_limit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@android:color/holo_green_dark"
                android:textSize="12sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1" >

            <TextView
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:text="total_up_limit:"
                android:textColor="#9400D3"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/total_up_limit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#9400D3"
                android:textSize="12sp" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:layout_width="130dp"
            android:layout_height="wrap_content"
            android:text="downloaded_ranges:"
            android:textColor="@android:color/holo_orange_dark"
            android:textSize="12sp" />
        <TextView 
            android:id="@+id/downloaded_ranges"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:textColor="@android:color/holo_orange_dark"
            />
        
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:layout_width="130dp"
            android:layout_height="wrap_content"
            android:text="overlap_dl_ranges:"
            android:textColor="@android:color/holo_orange_dark"
            android:textSize="12sp" />
        <TextView 
            android:id="@+id/overlap_dl_ranges"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:textColor="@android:color/holo_orange_dark"
            />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:layout_width="130dp"
            android:layout_height="wrap_content"
            android:text="uploadable_ranges:"
            android:textColor="@android:color/holo_orange_dark"
            android:textSize="12sp" />
        <TextView 
            android:id="@+id/uploadable_ranges"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:textColor="@android:color/holo_orange_dark"
            />
        
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:layout_width="130dp"
            android:layout_height="wrap_content"
            android:text="verified_ranges:"
            android:textColor="@android:color/holo_orange_dark"
            android:textSize="12sp" />
        <TextView
            android:id="@+id/verified_ranges"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@android:color/holo_orange_dark"
            android:textSize="12sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:layout_width="130dp"
            android:layout_height="wrap_content"
            android:text="stream_drag_pos:"
            android:textColor="@android:color/holo_orange_dark"
            android:textSize="12sp" />
        <TextView
            android:id="@+id/stream_drag_pos"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@android:color/holo_orange_dark"
            android:textSize="12sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:layout_width="130dp"
            android:layout_height="wrap_content"
            android:text="stream_send_pos:"
            android:textColor="@android:color/holo_orange_dark"
            android:textSize="12sp" />
        <TextView
            android:id="@+id/stream_send_pos"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@android:color/holo_orange_dark"
            android:textSize="12sp" />
    </LinearLayout>

</LinearLayout>